{
    "require": {
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/dotenv": "^5.0",
        "bluerhinos/phpmqtt": "^1.0",
        "cweagans/composer-patches": "^1.7",
        "influxdata/influxdb-client-php": "^1.9"
    },
    "extra": {
        "patches": {
            "bluerhinos/phpmqtt": {
                "TLS": "https://raw.githubusercontent.com/politsin/snipets/master/patch/phpMQTT.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    }
}
