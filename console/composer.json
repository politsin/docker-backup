{
  "require": {
    "cweagans/composer-patches": "^1.7",
    "bluerhinos/phpmqtt": "@dev",
    "symfony/console": "^5.0",
    "symfony/process": "^5.0",
    "symfony/dotenv": "^5.0"
  },
  "extra": {
    "patches": {
      "bluerhinos/phpmqtt": {
        "TLS": "https://raw.githubusercontent.com/politsin/snipets/master/patch/phpMQTT.patch"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  }
}
